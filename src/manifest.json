{
  "name": "Horn",
  "description": "Markdown text editor",
  "manifest_version": 2,
  "version": "0.100",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {
      "fileSystem": [
        "write",
        "retainEntries",
        "directory"
      ]
    },
    "syncFileSystem",
    "storage"
  ]
}
